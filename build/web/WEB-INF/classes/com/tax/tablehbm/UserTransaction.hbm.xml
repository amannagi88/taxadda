<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tax.tableentities.UserTransaction" table="tax_user_transaction">
        <id name="transactionId" type="java.lang.Integer">
            <column name="transaction_id" />
            <generator class="native" />
        </id>
        <property name="transactionIsStr" type="string">
            <column name="transaction_id_str" length="100" />
        </property>     
        <property name="userId" type="int">
            <column name="user_id"/>
        </property>   
        <property name="adhaarNo" type="string">
            <column name="adhaar_no"/>
        </property>   
        <property name="mobileNo" type="string">
            <column name="mobile_no"/>
        </property>   
        <property name="panNo" type="string">
            <column name="pan_no"/>
        </property>   
        <property name="activeStatus" type="int">
            <column name="active_status"/>
        </property>
        <property name="createDt" type="timestamp">
            <column name="create_dt" length="20" />
        </property>
        <property name="updateDt" type="timestamp">
            <column name="update_dt" length="20" />
        </property>
        
    </class>
</hibernate-mapping>
